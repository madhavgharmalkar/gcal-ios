git_url("git@github.com:madhavgharmalkar/better-gcal-certs.git")

storage_mode("git")

type("development") # The default type, can be: appstore, adhoc, enterprise or development

username = "madhavgharmalkar"
pat = ENV["PAT"]
to_encode = username + ":" + pat

git_basic_authorization Base64.strict_encode64(to_encode)

